        {
            "name": "Python: Arquivo Atual com RebuildSDE",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["--rebuildsde"],
            "console": "externalTerminal"
        },
        {
            "name": "Python: Arquivo Atual sem Parâmetros",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        },
        {
            "name": "Python: Arquivo Atual - debug rápido",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["--jsonfile", "eta.json", "--dontcreateschema"],
            "console": "externalTerminal"
        }
    ]
